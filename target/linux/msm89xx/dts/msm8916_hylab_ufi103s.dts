/dts-v1/;
#include "msm8916-ufi-common.dtsi"

/ {
	model = "UFI103S";
	compatible = "ufi,103s", "qcom,msm8916";

	gpio-keys {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;
		autorepeat;

		pinctrl-names = "default";
		pinctrl-0 = <&gpio_keys_default>;
		label = "GPIO Buttons";

		reset {
			label = "Restart";
			gpios = <&msmgpio 37 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};

	leds {
		pinctrl-names = "default";
		pinctrl-0 = <&msmgpio_leds>;

		compatible = "gpio-leds";

		wifi {
			label = "blue:wifi";
			gpios = <&msmgpio 20 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "phy0tx";
			default-state = "off";
		};

		led_internet: internet {
			label = "green:internet";
			gpios = <&msmgpio 21 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

		os {
			label = "red:os";
			gpios = <&msmgpio 22 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "heartbeat";
			default-state = "on";
		};

		sim_sel {
			label = "sim:sel";
			gpios = <&msmgpio 2 GPIO_ACTIVE_HIGH>;
			default-state = "on";
		};

		sim_en {
			label = "sim:en";
			gpios = <&msmgpio 1 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

		sim_sel2 {
			label = "sim:sel2";
			gpios = <&msmgpio 0 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

		sim_en2 {
			label = "sim:en2";
			gpios = <&msmgpio 3 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};


	};
};

&msmgpio {
	msmgpio_leds: msmgpio-leds {
		pins = "gpio0","gpio1","gpio2","gpio3","gpio20","gpio21","gpio22";
		function = "gpio";

		bias-disabled;
	};

	gpio_keys_default: gpio_keys_default {
		pins = "gpio37";
		function = "gpio";

		drive-strength = <8>;
		input-enable;
		bias-pull-up;
	};
};
